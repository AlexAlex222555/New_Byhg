#Область ПреобразованиеЦветовRGBHSV

// Возвращает таблицу соответствия web цветов и составляющих цветов - красного, зеленого, синего.
// 
// Возвращаемое значение:
//  ТаблицаЗначений - таблица соответствия web цветов и составляющих цветов.
//   *ВебЦвет - WebЦвета - цвет.
//   *ИмяВебЦвета - Строка - Имя веб цвета.
//   *Красный - Число - Тон красного цвета по RGB.
//   *Зеленый - Число - Тон зеленого цвета по RGB.
//   *Синий   - Число - Тон синего цвета по RGB.
//
Функция СоставляющиеВебЦветов() Экспорт
	
	Таблица = Новый ТаблицаЗначений();
	Таблица.Колонки.Добавить("ВебЦвет",     Новый ОписаниеТипов("Цвет"));
	Таблица.Колонки.Добавить("ИмяВебЦвета", Новый ОписаниеТипов("Строка",,,, Новый КвалификаторыСтроки(100, ДопустимаяДлина.Переменная)));
	Таблица.Колонки.Добавить("Красный",     Новый ОписаниеТипов("Число",,, Новый КвалификаторыЧисла(3, 0, ДопустимыйЗнак.Неотрицательный)));
	Таблица.Колонки.Добавить("Зеленый",     Новый ОписаниеТипов("Число",,, Новый КвалификаторыЧисла(3, 0, ДопустимыйЗнак.Неотрицательный)));
	Таблица.Колонки.Добавить("Синий",       Новый ОписаниеТипов("Число",,, Новый КвалификаторыЧисла(3, 0, ДопустимыйЗнак.Неотрицательный)));
	
	ВебЦветаВRGB = Отчеты.МониторЦелевыхПоказателей.ПолучитьМакет("ВебЦветаВRGB");
	ВебЦветаВRGB.КодЯзыка = КодЯзыкаКонфигурации();
	ОбластьСоответствиеWebЦветов = ВебЦветаВRGB.Области.СоответствиеWebЦветов;
	
	СоответствиеВебЦветов = ИменаВебЦветов();
	
	Для НомерСтроки = ОбластьСоответствиеWebЦветов.Верх По ОбластьСоответствиеWebЦветов.Низ Цикл
		
		ИмяВебЦвета = СокрЛП(ВебЦветаВRGB.Область("R"+НомерСтроки+"C3:R"+НомерСтроки+"C2").Текст);
		ВебЦвет = СоответствиеВебЦветов.Получить(ИмяВебЦвета);
		Если ВебЦвет = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		НоваяСтрока = Таблица.добавить();
		НоваяСтрока.ИмяВебЦвета = ИмяВебЦвета;
		НоваяСтрока.Красный     = Число(ВебЦветаВRGB.Область("R"+НомерСтроки+"C3:R"+НомерСтроки+"C3").Текст);
		НоваяСтрока.Зеленый     = Число(ВебЦветаВRGB.Область("R"+НомерСтроки+"C4:R"+НомерСтроки+"C4").Текст);
		НоваяСтрока.Синий       = Число(ВебЦветаВRGB.Область("R"+НомерСтроки+"C5:R"+НомерСтроки+"C5").Текст);
		НоваяСтрока.ВебЦвет     = ВебЦвет;
		
	КонецЦикла;
	Таблица.Индексы.Добавить("ВебЦвет");
	
	Возврат Таблица;
КонецФункции

