	Для Каждого Элемент из Коды Цикл
		СправочникОбъект = Элемент.Ключ.ПолучитьОбъект();
		СправочникОбъект.Код = СокрЛП(СправочникОбъект.Код) + "00";
		СправочникОбъект.Записать();
	КонецЦикла;	

	Для Каждого Элемент из Коды Цикл
		СправочникОбъект = Элемент.Ключ.ПолучитьОбъект();
		СправочникОбъект.Код = Элемент.Значение;
		СправочникОбъект.Записать();
	КонецЦикла;	
	
